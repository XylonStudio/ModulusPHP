{
  "name": "modulusphp/modulusphp",
  "description": "A lightweight PHP Framework",
  "keywords": ["framework", "modulusphp"],
  "version": "1.3.04",
  "license": "MIT",
  "type": "project",
  "authors": [
      {
          "name": "Donald Pakkies",
          "email": "donaldpakkies@gmail.com"
      }
  ],
  "require": {
      "php": ">=7.0.0",
      "atlantisphp/swish": ">=1.0",
      "filp/whoops": ">=2.2"
  },
  "autoload": {
      "psr-4": {
          "App\\": "app/",
          "Storage\\": "storage/"
      },
      "files": ["bootstrap/app.php"]
  },
  "scripts": {
      "post-root-package-install": [
          "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
      ],
      "post-install-cmd": [
          "composer dump-autoload -o"
      ],
      "post-update-cmd": [
          "composer dump-autoload -o"
      ]
  },
  "config": {
      "preferred-install": "dist",
      "sort-packages": true,
      "optimize-autoloader": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require-dev": {
  }
}
