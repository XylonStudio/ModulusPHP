{
  "name": "modulusphp/modulusphp",
  "description": "A lightweight PHP Framework",
  "keywords": ["framework", "modulusphp"],
  "version": "1.9.1",
  "license": "MIT",
  "type": "project",
  "authors": [{
    "name": "Donald Pakkies",
    "email": "donaldpakkies@gmail.com"
  }],
  "require": {
    "php": ">=7.3.0",
    "modulusphp/framework": "^1.9.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": ["bootstrap/app.php"]
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php modulus key:generate",
      "@php modulus docsify:link"
    ],
    "post-install-cmd": [
      "composer dump-autoload -o"
    ],
    "post-update-cmd": [
      "composer dump-autoload -o"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require-dev": {}
}
